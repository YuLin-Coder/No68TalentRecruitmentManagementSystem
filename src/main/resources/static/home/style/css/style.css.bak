.savePassword td.label{vertical-align:top;padding-top:22px;}
.savePassword td.c7{ padding-left:15px;}
.savePassword td input[type="password"]{width: 398px; border: 2px solid #f1f1f1; margin:0 0 0 10px; }
.savePassword td input[type="submit"]{ height: 35px; color: #fff; background: #019875; font-size: 18px;padding:0 20px;line-height: 35px; margin-left: 10px;}
